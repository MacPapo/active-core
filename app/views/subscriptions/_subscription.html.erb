<div id="<%= dom_id subscription %>">
  <p>
    <strong>Start:</strong>
    <%= subscription.start %>
  </p>

  <p>
    <strong>End:</strong>
    <%= subscription.end %>
  </p>

  <p>
    <strong>User:</strong>
    <%= subscription.user_id %>
  </p>

  <p>
    <strong>Course:</strong>
    <%= subscription.course_id %>
  </p>

  <p>
    <strong>Subscription type:</strong>
    <%= subscription.subscription_type_id %>
  </p>

  <p>
    <strong>State:</strong>
    <%= subscription.state %>
  </p>

</div>
